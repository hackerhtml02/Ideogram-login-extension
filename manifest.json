{
  "manifest_version": 2,
  "name": "Auto Login Extension Made By Muhammad Haris",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "https://ideogram.ai/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://ideogram.ai/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png"
  }
}
